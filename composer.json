{
    "name": "bristol-su/typeform-service",
    "description": "A third party service for typeform",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Toby Twigger",
            "email": "toby.twigger@bristol.ac.uk"
        }
    ],
    "require": {
        "php": "^8.0",
        "bristol-su/support": "^5.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.19",
        "laravel/legacy-factories": "^1.1",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BristolSU\\Service\\Typeform\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BristolSU\\Service\\Tests\\Typeform\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BristolSU\\Service\\Typeform\\TypeformServiceProvider"
            ]
        }
    }
}
